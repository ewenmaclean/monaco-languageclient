{
  "private": true,
  "name": "browser-example",
  "version": "0.17.4",
  "dependencies": {
    "file-loader": "^4.3.0",
    "monaco-languageclient": "^0.18.0",
    "vscode-json-languageservice": "^4.1.9",
    "vscode-languageserver-types": "^3.16.0",
    "css-loader": "^0.28.11",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "compile": "tsc",
    "watch": "tsc -w",
    "clean": "npx shx rm -fr lib",
    "copy": "shx cp src/index.html lib/index.html",
    "build": "npm run compile && webpack && npm run copy",
    "update:file-deps": "npm run clean:file-deps && npm install",
    "clean:file-deps": "npm run clean:monaco-languageclient",
    "update:monaco-languageclient": "npm run clean:monaco-languageclient && npm install",
    "clean:monaco-languageclient": "npx shx -fr node_modules/monaco-languageclient"
  }
}
